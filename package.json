{
  "name": "zame-particles",
  "version": "2.0.0",
  "description": "Particle emitter for OpenFL compatible with Particle Designer",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-lib",
    "_build": "npm run build-lib && npm run build-dist",
    "build-dist": "webpack --config webpack.dev.js && webpack --config webpack.prod.js",
    "build-lib": "cd scripts && haxe build.hxml",
    "prepublishOnly": "npm run build",
    "install": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/restorer/zame-haxe-particles.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/restorer/zame-haxe-particles/issues"
  },
  "peerDependencies": {
    "openfl": "^7.1.2",
    "webpack": "^2.0.0 || ^3.0.0"
  },
  "homepage": "https://github.com/restorer/zame-haxe-particles#readme",
  "devDependencies": {
    "haxe": "^5.0.0",
    "hxgenjs": "kevinresol/hxgenjs",
    "openfl": "^7.1.2",
    "lime": "openfl/lime#develop"
  }
}
